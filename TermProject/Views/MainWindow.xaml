<Window x:Class="TermProject.Views.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:prism="http://prismlibrary.com/"
        prism:ViewModelLocator.AutoWireViewModel="True"
        Title="{Binding Title}" Height="720" Width="1024">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="512*"/>
            <ColumnDefinition Width="512*"/>
        </Grid.ColumnDefinitions>
        <ContentControl prism:RegionManager.RegionName="ImageScreen" />
        <ContentControl prism:RegionManager.RegionName="MapScreen" Grid.Column="1" />
    </Grid>
</Window>
